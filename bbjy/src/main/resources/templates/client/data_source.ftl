<!DOCTYPE html><html lang="en"><head>    <meta charset="UTF-8">    <meta name="description" content=""/>    <meta name="keywords" content=""/>    <meta name="author" content=""/>    <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1"/>    <title>行销资源<#if catTitle??>-${catTitle!''}<#else></#if></title>    <!--[if lt IE 9]>    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>    <![endif]-->    <link rel="shortcut icon" href="/client/crmimages/icon.ico" />    <link rel="stylesheet" href="/client/css/crmglobal.css"/>    <link rel="stylesheet" href="/client/css/crmsite1.css"/>    <script src="/client/js/jquery-1.11.0.js" type="text/javascript"></script>    <script src="/client/js/crmcommon.js" type="text/javascript"></script></head><body><!-- crm头部 --><#include "/client/common_header.ftl" /><script>    $(function(){        showdetaildialog("#productlist .dialoghref","datedialog");         $('div > .parentnode').togglemenu1('div.children');    });        function sourcePage(catId,page){    	location.href='/data/source?catId='+catId+'&page='+page;    }        function selectSourcePage(catId,totalPage){    	var page = $("#select_page").val()-1;    	var totalPage = totalPage -1;    	if(page == ""){    		page = 0;    	}else if(page < 1){    		page= 0;    	}else if(page > totalPage){    		page = totalPage;    	}    	location.href='/data/source?catId='+catId+'&page='+page;    }</script><div class="container" id="contentwrap">    <div class="leftnav">        <span class="computericon">行销资源</span>        <#if cat_list??>        <#list cat_list as cat>        	<div class="havechild">        	<span <#if parentId?? && parentId == cat.id>class=" parentnode current"</#if>>${cat.title!''}</span>        	<div class="children" style="display: block;">        		<#if ("sub_"+cat.id)?eval??>		        	<#assign subCat =  ("sub_"+cat.id)?eval>		        	<#list subCat as sub>		        	<a href="/data/source?catId=${sub.id?c}" title="" <#if catId?? && catId == sub.id>class="current"</#if>>		        	<#if sub.title??>	                    <#if sub.title?length gt 30>	                    	${sub.title[0..29]}...	                    <#else>	                    	${sub.title}	                    </#if>                    </#if>		        	</a>	        		</#list>	    		</#if>               </div>        	</div>        </#list>        </#if>    </div>    <div class="right-content">        <span class="contenttitle contenttitlebg"><#if catTitle??>${catTitle!''}<#else>行销资源</#if></span>        <div class="incontent">            <ul class="ullistitem" id="productlist">                <li class="thli">产品名称</li>                <#if info_page??>                <#list info_page.content as article>               		<li>${article_index+1}.<a href="javascript:;" class="dialoghref" articleId="${article.id?c}"   title="${article.title!''}">               		<#if article.title??>	                    <#if article.title?length gt 40>	                    	${article.title[0..39]}...	                    <#else>	                    	${article.title}	                    </#if>                    </#if>               		</a></li>            	</#list>                </#if>              </ul>            <div class="listitem" id="listdetailitem">            </div>            <div class="page">        	<#if info_page??>        	<#assign PAGE_DATA = info_page>                <label class="pagesite">                当前第${PAGE_DATA.number*size+1}到${PAGE_DATA.number*size+PAGE_DATA.numberOfElements}条，共${PAGE_DATA.totalElements}条</label>                <span class="disabled" onclick="javascript:sourcePage(${catId!''},'0')" style="cursor:pointer;">首页</span>                <#assign continueEnter=false>            	<#if PAGE_DATA.number+1 == 1>                	<span class="disabled">&lt; 上一页</span>            	<#else>               		 <a href="javascript:sourcePage(${catId!''},'${PAGE_DATA.number-1}')" class="lastPage">&lt;上一页</a>            	</#if>            					<#if PAGE_DATA.totalPages gt 0>                <#list 1..PAGE_DATA.totalPages as page>                    <#if page <= 3 || (PAGE_DATA.totalPages-page) < 3 || (PAGE_DATA.number+1-page)?abs<3 >                        <#if page == PAGE_DATA.number+1>                            <span class="current">${page}</span>                        <#else>                            <a href="javascript:sourcePage(${catId!''},'${page-1}')" tcdNumber>${page}</a>                        </#if>                        <#assign continueEnter=false>                    <#else>                        <#if !continueEnter>                            ...                            <#assign continueEnter=true>                        </#if>                    </#if>                </#list>            	</#if>                            <#if PAGE_DATA.number+1 == PAGE_DATA.totalPages || PAGE_DATA.totalPages==0>	                <span class="disabled">下一页&gt;</span>	            <#else>	                <a href="javascript:sourcePage(${catId!''},'${PAGE_DATA.number+1}')" class="nextPage">下一页&gt;</a>	            </#if>                <input type="text" id="select_page" value="${PAGE_DATA.number+1}"/>                <input type="button" value="确定" style="cursor:pointer;" onclick="javascript:selectSourcePage(${catId!''},${PAGE_DATA.totalPages})"/>            </#if>            </div>        </div>    </div>    <div class="dialogwrap dialog-detailinfo" id="datedialog">		<#include "/client/data_detail.ftl">    </div></div></body></html>